    "version:patch": "pwsh -NoProfile -ExecutionPolicy Bypass -File ./scripts/Update-Version.ps1 -Type patch",
    "version:set": "pwsh -NoProfile -ExecutionPolicy Bypass -File ./scripts/Update-Version.ps1 -NewVersion",
    "test": "echo \"No tests configured yet\" && exit 0",
    "lint": "echo \"Linting userscript...\" && exit 0",
    "build": "echo \"âœ… Userscript ready for deployment\" && echo \"ğŸ“¦ File: disable-yt-hotkeys.user.js\"",
